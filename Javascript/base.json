{
  "extends": "eslint:recommended",
  "rules": {
    "arrow-parens": 0,
    "curly": 0,
    "forin": 0,
    "indent": ["error", 2, { "SwitchCase": 1 }],
    "interface-name": 0,
    "valid-jsdoc": 2,
    "member-access": 0,
    "no-bitwise": 0,
    "no-console": 1,
    "no-reference": 0,
    "no-string-literal": 0,
    "no-trailing-whitespace": 0,
    "no-unused-new": 0,
    "no-var-requires": 0,
    "object-literal-sort-keys": 0,
    "only-arrow-functions": 0,
    "ordered-imports": 0,
    "prefer-for-of": 0,
    "trailing-comma": 0
  }
}
